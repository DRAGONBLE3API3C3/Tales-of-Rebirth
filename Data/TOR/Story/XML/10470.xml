    <Entry>
      <PointerOffset>4910</PointerOffset>
      <JapaneseText>&lt;voice:300000ad&gt;……&lt;Claire&gt;……？</JapaneseText>
      <EnglishText>&lt;voice:300000AD&gt;&lt;Claire&gt;?</EnglishText>
      <Notes/>
      <Id>105</Id>
      <Status>Editing</Status>
    </Entry>
