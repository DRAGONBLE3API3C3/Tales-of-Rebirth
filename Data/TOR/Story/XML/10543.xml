    <Entry>
      <PointerOffset>1450</PointerOffset>
      <VoiceId>2c00007d</VoiceId>
      <JapaneseText>あの娘も、悪くないですね。
キレイな服を着せて、
身なりを整えてさしあげなさい。</JapaneseText>
      <EnglishText/>
      <SpeakerId>0</SpeakerId>
      <Notes/>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1488</PointerOffset>
      <VoiceId>2c00007e</VoiceId>
      <JapaneseText>…………</JapaneseText>
      <EnglishText/>
      <SpeakerId>1</SpeakerId>
      <Notes/>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2208</PointerOffset>
      <VoiceId>2c0000fd</VoiceId>
      <JapaneseText>何があった!?</JapaneseText>
      <EnglishText/>
      <SpeakerId>2</SpeakerId>
      <Notes/>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2306</PointerOffset>
      <VoiceId>2c0000fe</VoiceId>
      <JapaneseText>客に変装した盗賊たちが……</JapaneseText>
      <EnglishText/>
      <SpeakerId>1</SpeakerId>
      <Notes/>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2335</PointerOffset>
      <VoiceId>2c0000ff</VoiceId>
      <JapaneseText>&lt;Claire&gt;はどこだ!?</JapaneseText>
      <EnglishText/>
      <SpeakerId>2</SpeakerId>
      <Notes/>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2357</PointerOffset>
      <VoiceId>2c000100</VoiceId>
      <JapaneseText>……&lt;Claire&gt;？　娘は……
もう……いない……</JapaneseText>
      <EnglishText/>
      <SpeakerId>1</SpeakerId>
      <Notes/>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2384</PointerOffset>
      <VoiceId>2c000101</VoiceId>
      <JapaneseText>いない？　どういう意味だ!!</JapaneseText>
      <EnglishText/>
      <SpeakerId>2</SpeakerId>
      <Notes/>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2406</PointerOffset>
      <VoiceId>2c000102</VoiceId>
      <JapaneseText>うぅ……</JapaneseText>
      <EnglishText/>
      <SpeakerId>1</SpeakerId>
      <Notes/>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2524</PointerOffset>
      <VoiceId>2c000103</VoiceId>
      <JapaneseText>気絶したみたいです。</JapaneseText>
      <EnglishText/>
      <SpeakerId>3</SpeakerId>
      <Notes/>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2553</PointerOffset>
      <VoiceId>2c000104</VoiceId>
      <JapaneseText>くそっ……&lt;Claire&gt;はどこだ!?</JapaneseText>
      <EnglishText/>
      <SpeakerId>2</SpeakerId>
      <Notes/>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2827</PointerOffset>
      <VoiceId>2c000126</VoiceId>
      <JapaneseText>お腹立ちはごもっともです。
……おいくらですか？</JapaneseText>
      <EnglishText/>
      <SpeakerId>0</SpeakerId>
      <Notes/>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2872</PointerOffset>
      <VoiceId>2c000127</VoiceId>
      <JapaneseText>なに？</JapaneseText>
      <EnglishText/>
      <SpeakerId>2</SpeakerId>
      <Notes/>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2894</PointerOffset>
      <VoiceId>2c000128</VoiceId>
      <JapaneseText>お詫びと言ってはなんですが、
お金ならいくらでも差し上げます。
お望みの額をおっしゃって……</JapaneseText>
      <EnglishText/>
      <SpeakerId>0</SpeakerId>
      <Notes/>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2951</PointerOffset>
      <VoiceId>2c000129</VoiceId>
      <JapaneseText>いいかげんにしろ!!
金、金、カネカネ……
それが何だ!!</JapaneseText>
      <EnglishText/>
      <SpeakerId>4</SpeakerId>
      <Notes/>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3010</PointerOffset>
      <VoiceId>2c00012a</VoiceId>
      <JapaneseText>だいたい、何のためにクレアさんを
かどわかそうとしやがったんだ!?</JapaneseText>
      <EnglishText/>
      <SpeakerId>4</SpeakerId>
      <Notes/>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3077</PointerOffset>
      <VoiceId>2c00012b</VoiceId>
      <JapaneseText>ほっほっほっ……お客様に
ご満足いただくためでございます。</JapaneseText>
      <EnglishText/>
      <SpeakerId>0</SpeakerId>
      <Notes/>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3144</PointerOffset>
      <VoiceId>2c00012c</VoiceId>
      <JapaneseText>なんだと!?</JapaneseText>
      <EnglishText/>
      <SpeakerId>4</SpeakerId>
      <Notes/>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3166</PointerOffset>
      <VoiceId>2c00012d</VoiceId>
      <JapaneseText>もちろん、それなりの余禄は
ございますけれども……
ほほほ……</JapaneseText>
      <EnglishText/>
      <SpeakerId>0</SpeakerId>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3261</PointerOffset>
      <VoiceId>2c00012f</VoiceId>
      <JapaneseText>このヤロウッ!!</JapaneseText>
      <EnglishText/>
      <SpeakerId>4</SpeakerId>
      <Notes/>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3334</PointerOffset>
      <VoiceId>2c000130</VoiceId>
      <JapaneseText>待ちなさいよ、&lt;Tytree&gt;！
今ここで殴ったら……</JapaneseText>
      <EnglishText/>
      <SpeakerId>5</SpeakerId>
      <Notes/>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3374</PointerOffset>
      <VoiceId>2c000131</VoiceId>
      <JapaneseText>一発殴らせろ!!</JapaneseText>
      <EnglishText/>
      <SpeakerId>4</SpeakerId>
      <Notes/>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3727</PointerOffset>
      <VoiceId>2c000132</VoiceId>
      <JapaneseText>&lt;Veigue&gt;！</JapaneseText>
      <EnglishText/>
      <SpeakerId>6</SpeakerId>
      <Notes/>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3792</PointerOffset>
      <VoiceId>2c000133</VoiceId>
      <JapaneseText>いたたた……あなたたち……
そんな事をしていいと
思っているのですか？</JapaneseText>
      <EnglishText/>
      <SpeakerId>0</SpeakerId>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3863</PointerOffset>
      <VoiceId>2c000134</VoiceId>
      <JapaneseText>……クウ・ホウ、何をしている！
早くこいつらをたたき出してしまえ！</JapaneseText>
      <EnglishText/>
      <SpeakerId>0</SpeakerId>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>17376</PointerOffset>
