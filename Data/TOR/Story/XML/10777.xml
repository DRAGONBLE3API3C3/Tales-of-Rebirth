      <EnglishText/>
      <SpeakerId>14</SpeakerId>
      <Notes/>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>285</PointerOffset>
      <JapaneseText>こんなところに……
