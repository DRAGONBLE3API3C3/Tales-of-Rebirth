      <EnglishText/>
      <SpeakerId>14</SpeakerId>
      <Notes/>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>304</PointerOffset>
      <JapaneseText>初めてのアイテム会話</JapaneseText>
