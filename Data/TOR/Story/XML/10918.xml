    <Entry>
      <PointerOffset>1460</PointerOffset>
      <VoiceId>3300003f</VoiceId>
      <JapaneseText>あんたが……
ナイラさん……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1482</PointerOffset>
      <VoiceId>33000040</VoiceId>
      <JapaneseText>はい。私がナイラですが、
どのようなご用件でしょうか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1513</PointerOffset>
      <VoiceId>33000041</VoiceId>
      <JapaneseText>これと同じものを使う占い師が、
ここにいるって……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1596</PointerOffset>
      <VoiceId>33000042</VoiceId>
      <JapaneseText>あんたなのね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1618</PointerOffset>
      <VoiceId>33000043</VoiceId>
      <JapaneseText>……あ、あなた……
一体、これをどこで……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1649</PointerOffset>
      <VoiceId>33000044</VoiceId>
      <JapaneseText>知らないわ……
それを知るために、
ここに来たのよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1671</PointerOffset>
      <VoiceId>33000045</VoiceId>
      <JapaneseText>……あ、あなた……
お名前は？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1701</PointerOffset>
      <VoiceId>33000046</VoiceId>
      <JapaneseText>……&lt;Hilda&gt;。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1741</PointerOffset>
      <VoiceId>33000047</VoiceId>
      <JapaneseText>……もしかして、あなた……
……ハーフ……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1838</PointerOffset>
      <VoiceId>33000048</VoiceId>
      <JapaneseText>……どうしてそれを!?</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1860</PointerOffset>
      <VoiceId>33000049</VoiceId>
      <JapaneseText>……やっぱり……
&lt;Hilda&gt;……&lt;Hilda&gt;……
生きていたのね……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1891</PointerOffset>
      <VoiceId>3300004a</VoiceId>
      <JapaneseText>……何よ……？
どういう事……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1913</PointerOffset>
      <VoiceId>3300004b</VoiceId>
      <JapaneseText>&lt;Hilda&gt;……私が……
私が……あなたの母親よ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1935</PointerOffset>
      <VoiceId>3300004c</VoiceId>
      <JapaneseText>あんたが……私の母親……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1965</PointerOffset>
      <VoiceId>3300004d</VoiceId>
      <JapaneseText>い、いきなりビンゴかよ……
よかったな、&lt;Hilda&gt;!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1987</PointerOffset>
      <VoiceId>3300004e</VoiceId>
      <JapaneseText>&lt;Hilda&gt;……私の&lt;Hilda&gt;……
あなたの顔を、よく見せて……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2028</PointerOffset>
      <VoiceId>3300004f</VoiceId>
      <JapaneseText>…………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2068</PointerOffset>
      <VoiceId>33000050</VoiceId>
      <JapaneseText>ふたつの角を……
頂く女……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2147</PointerOffset>
      <VoiceId>33000051</VoiceId>
      <JapaneseText>何するの!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2177</PointerOffset>
      <VoiceId>33000052</VoiceId>
      <JapaneseText>……今すぐ……この村から……
出ていきなさい……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2208</PointerOffset>
      <VoiceId>33000053</VoiceId>
      <JapaneseText>……えっ……
何ですって……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2230</PointerOffset>
      <VoiceId>33000054</VoiceId>
      <JapaneseText>そう……出ていきなさい……
……出ていって……!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2261</PointerOffset>
      <VoiceId>33000055</VoiceId>
      <JapaneseText>何よ？　どういう事……!?</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2290</PointerOffset>
      <VoiceId>33000056</VoiceId>
      <JapaneseText>ハーフの娘なんて……
見たくもないわ……!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2337</PointerOffset>
      <VoiceId>33000057</VoiceId>
      <JapaneseText>…………！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2373</PointerOffset>
      <VoiceId>33000058</VoiceId>
      <JapaneseText>……そう……そうよね……
私は……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2404</PointerOffset>
      <VoiceId>33000059</VoiceId>
      <JapaneseText>&lt;Hilda&gt;さん……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2442</PointerOffset>
      <VoiceId>3300005a</VoiceId>
      <JapaneseText>ふふっ……私がバカだった……
私は、あんたに一度、
見捨てられてるんだもんね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2481</PointerOffset>
      <VoiceId>3300005b</VoiceId>
      <JapaneseText>……私は今までずっと、
両親はいないものだって、
自分に言い聞かせてきた……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2512</PointerOffset>
      <VoiceId>3300005c</VoiceId>
      <JapaneseText>どうしてだかわかる？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2534</PointerOffset>
      <JapaneseText>…………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2556</PointerOffset>
      <VoiceId>3300005d</VoiceId>
      <JapaneseText>そのほうが、
少しは哀しい気持ちになる、
少しは憎しみも弱くなる、
そんな気がしたからよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2587</PointerOffset>
      <VoiceId>3300005e</VoiceId>
      <JapaneseText>……本当にバカよね……
私も……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2609</PointerOffset>
      <VoiceId>3300005f</VoiceId>
      <JapaneseText>今すぐ……
私の前から消えて……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2640</PointerOffset>
      <VoiceId>33000060</VoiceId>
      <JapaneseText>……言われなくても、
出ていくわよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2716</PointerOffset>
      <VoiceId>33000061</VoiceId>
      <JapaneseText>どういう事なんだよ!?
さっきの涙は何だったんだよ!?</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2738</PointerOffset>
      <VoiceId>33000062</VoiceId>
      <JapaneseText>……すみません。
出ていってください……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2760</PointerOffset>
      <VoiceId>33000063</VoiceId>
      <JapaneseText>&lt;Hilda&gt;を……あの子を連れて……
すぐにこの村から！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2791</PointerOffset>
      <VoiceId>33000064</VoiceId>
      <JapaneseText>待ってくれよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2864</PointerOffset>
      <VoiceId>33000065</VoiceId>
      <JapaneseText>&lt;Hilda&gt;が心配だ。
今はあいつを……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3150</PointerOffset>
      <VoiceId>330000eb</VoiceId>
      <JapaneseText>&lt;Hilda&gt;さん……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3296</PointerOffset>
      <VoiceId>330000ec</VoiceId>
      <JapaneseText>これは……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3318</PointerOffset>
      <VoiceId>330000ed</VoiceId>
      <JapaneseText>もし、自分が死んだ後、
娘が来るような事があったら、
これを渡して欲しいって……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3340</PointerOffset>
      <VoiceId>330000ee</VoiceId>
      <JapaneseText>私みたいなおばあさんに
お願いするなんてヘンよねぇ……
まるで、自分が死ぬのが
わかってたみたい……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3396</PointerOffset>
      <VoiceId>330000ef</VoiceId>
      <JapaneseText>&lt;Hilda&gt;……
どこかへとさらわれていった
あなたが……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3424</PointerOffset>
      <VoiceId>330000f1</VoiceId>
      <JapaneseText>……ここを訪ねてきてくれた時の
ために、この手紙を残します。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3445</PointerOffset>
      <VoiceId>330000f2</VoiceId>
      <JapaneseText>やっとあなたがここへ来てくれたのに、
直接謝る事ができなくて、
本当にごめんなさい。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3466</PointerOffset>
      <VoiceId>330000f3</VoiceId>
      <JapaneseText>ヒューマのお父さん……アムジルと
ガジュマの私の間に生まれたあなたが、
自分の身体の事でいつか苦しむだろう事は
予想していました。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3487</PointerOffset>
      <VoiceId>330000f4</VoiceId>
      <JapaneseText>それでも、私たちはあなたを生みました。
私たちが見つけた愛のすばらしさに、
生命を吹き込み、喜びを分かち合うためです
……あなたと。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3508</PointerOffset>
      <VoiceId>330000f5</VoiceId>
      <JapaneseText>ハーフと呼ばれ、忌み嫌われる
その身体を、あなたは呪い、
私たちを恨んでいるかもしれません。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3529</PointerOffset>
      <VoiceId>330000f6</VoiceId>
      <JapaneseText>でもあなたは、私たちの誇りです。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3550</PointerOffset>
      <VoiceId>330000f7</VoiceId>
      <JapaneseText>あなたはハーフなんかじゃない。
半分なんかじゃない……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3571</PointerOffset>
      <VoiceId>330000f8</VoiceId>
      <JapaneseText>お父さんと私のふたつの命を
併せ持つ存在だからです……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3592</PointerOffset>
      <VoiceId>330000f9</VoiceId>
      <JapaneseText>だから……胸を張って生きてください。
ヒューマでもガジュマでもハーフでもない
……&lt;Yellow&gt;ヒト&lt;White&gt;として。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3625</PointerOffset>
      <VoiceId>330000fa</VoiceId>
      <JapaneseText>&lt;Yellow&gt;ヒト&lt;White&gt;として……
あなたの母、ナイラ……
私の……愛する……&lt;Hilda&gt;……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3647</PointerOffset>
      <VoiceId>330000fb</VoiceId>
      <JapaneseText>……なによ……こんな手紙……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3722</PointerOffset>
      <VoiceId>330000fc</VoiceId>
      <JapaneseText>ふたつの角を頂く女……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>6</SpeakerId>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3779</PointerOffset>
      <VoiceId>330000fd</VoiceId>
      <JapaneseText>「寄る辺なき者たちが集いし北の村で、
　ふたつの角を頂く女、
　古き星をその手に掴む時、
　天命尽き大地に還る」……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>6</SpeakerId>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3809</PointerOffset>
      <VoiceId>330000fe</VoiceId>
      <JapaneseText>これって、ナイラさんの字かしら？
なんだか、ナイラさんの事を暗示
しているように思えるんだけど……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>6</SpeakerId>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3849</PointerOffset>
      <VoiceId>330000ff</VoiceId>
      <JapaneseText>古き星って、タロットの事だろ？
&lt;Hilda&gt;とも言えなくないか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3889</PointerOffset>
      <VoiceId>33000100</VoiceId>
      <JapaneseText>……ナイラさんは自分の死を、
占いで予言してたんじゃない
でしょうか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3911</PointerOffset>
      <VoiceId>33000101</VoiceId>
      <JapaneseText>でも、突然現れた&lt;Hilda&gt;さんも
自分と同じ古いタロットを持っていて、
「ふたつの角を頂く女」だと気づいた。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4033</PointerOffset>
      <VoiceId>33000102</VoiceId>
      <JapaneseText>……だから、ナイラさんは
&lt;Hilda&gt;のタロットを
破り捨てたという事か？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4070</PointerOffset>
      <VoiceId>33000103</VoiceId>
      <JapaneseText>ああ……その上で&lt;Hilda&gt;を拒み、
予言が果たされるこの村を去るように
仕向けたんだろう……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>7</SpeakerId>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4092</PointerOffset>
      <VoiceId>33000104</VoiceId>
      <JapaneseText>……だったら何よ……
私の身代わりになって死ぬなんて、
どうかしてるわ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4114</PointerOffset>
      <VoiceId>33000105</VoiceId>
      <JapaneseText>私が望んでいたのは……
こんな手紙でも、
命を救ってもらう事でもない!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4145</PointerOffset>
      <VoiceId>33000106</VoiceId>
      <JapaneseText>私が欲しかったのは……
あの人に言いたかったのは……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4167</PointerOffset>
      <VoiceId>33000107</VoiceId>
      <JapaneseText>……私……一度もあの人の事、
お母さんって呼んだ事なかった……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>71</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4195</PointerOffset>
      <VoiceId>33000108</VoiceId>
      <JapaneseText>……お母さん……お母さん……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>72</Id>
      <Status>To Do</Status>
    </Entry>
