    <Entry>
      <PointerOffset>1390</PointerOffset>
      <VoiceId>16000044</VoiceId>
      <JapaneseText>トーマ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1412</PointerOffset>
      <VoiceId>16000045</VoiceId>
      <JapaneseText>そこの小屋に
ヒューマの娘がいるだろう。
命が惜しければ、娘を渡せ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1452</PointerOffset>
      <VoiceId>16000046</VoiceId>
      <JapaneseText>誰が渡すかよ!!
おまえのほうこそ、姉貴や
クレアさんを返しやがれ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1483</PointerOffset>
      <VoiceId>16000047</VoiceId>
      <JapaneseText>ほう、そうか……
ならば、死ね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1746</PointerOffset>
      <VoiceId>16000048</VoiceId>
      <JapaneseText>ぐああぁぁッ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1763</PointerOffset>
      <JapaneseText>ぬうううッ……!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1780</PointerOffset>
      <JapaneseText>わああああッ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1798</PointerOffset>
      <JapaneseText>きゃあぁぁッ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1816</PointerOffset>
      <JapaneseText>うわぁぁぁぁぁッ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2193</PointerOffset>
      <VoiceId>1600004e</VoiceId>
      <JapaneseText>……&lt;Hilda&gt;……おまえ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2224</PointerOffset>
      <VoiceId>1600004f</VoiceId>
      <JapaneseText>だから言っただろう？
命が惜しければ娘を渡せ、と。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2261</PointerOffset>
      <VoiceId>16000050</VoiceId>
      <JapaneseText>あなたも能力者……!?</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2300</PointerOffset>
      <VoiceId>16000051</VoiceId>
      <JapaneseText>ええ……&lt;Blue&gt;王の盾&lt;White&gt;のね。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2337</PointerOffset>
      <VoiceId>16000052</VoiceId>
      <JapaneseText>どういうコト……？
じゃあ、サニイタウンで会った
&lt;Hilda&gt;のお母さんは……？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2368</PointerOffset>
      <VoiceId>16000053</VoiceId>
      <JapaneseText>お金で雇っただけで、
何の関係もないわ。
私に母親なんていない……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2405</PointerOffset>
      <VoiceId>16000054</VoiceId>
      <JapaneseText>クレアや……
セレーナさんの話も……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2436</PointerOffset>
      <VoiceId>16000055</VoiceId>
      <JapaneseText>会った事もないわ。
名前を利用しただけ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2467</PointerOffset>
      <VoiceId>16000056</VoiceId>
      <JapaneseText>だまし討ちなんて……
汚ねえぞ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2498</PointerOffset>
      <VoiceId>16000057</VoiceId>
      <JapaneseText>だまされるほうが悪いのよ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2538</PointerOffset>
      <VoiceId>16000058</VoiceId>
      <JapaneseText>許せねえ……
絶対、許せねえッ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2739</PointerOffset>
      <VoiceId>16000059</VoiceId>
      <JapaneseText>&lt;Hilda&gt;！
こいつらを皆殺しにしろ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2975</PointerOffset>
      <VoiceId>1600005a</VoiceId>
      <JapaneseText>くっ！　だから言っただろうが。
くだらん意地のために
大事な角を折ったりするから
こんなふがいない事になるんだ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2998</PointerOffset>
      <VoiceId>1600005b</VoiceId>
      <JapaneseText>……角なんて……
私には必要ない……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3020</PointerOffset>
      <VoiceId>1600005c</VoiceId>
      <JapaneseText>もういい！　&lt;Red&gt;混ざり物&lt;White&gt;め。
力も出せず、任務もまともに
果たせないおまえに、もう用はない！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3043</PointerOffset>
      <VoiceId>1600005d</VoiceId>
      <JapaneseText>私は……まだやれる!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3073</PointerOffset>
      <VoiceId>1600005e</VoiceId>
      <JapaneseText>角とか、混ざり物とか、
何を言ってるんだ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3094</PointerOffset>
      <VoiceId>1600005f</VoiceId>
      <JapaneseText>……まさか、&lt;Hilda&gt;は？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3116</PointerOffset>
      <VoiceId>16000060</VoiceId>
      <JapaneseText>そのとおり、&lt;Hilda&gt;は……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3147</PointerOffset>
      <VoiceId>16000061</VoiceId>
      <JapaneseText>黙れ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3214</PointerOffset>
      <VoiceId>16000062</VoiceId>
      <JapaneseText>ふん、こいつらにも
おまえのみじめな正体を
教えてやれ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3290</PointerOffset>
      <VoiceId>16000063</VoiceId>
      <JapaneseText>いやぁぁぁ！
やめて……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3549</PointerOffset>
      <VoiceId>16000064</VoiceId>
      <JapaneseText>ヒューマに、角が……!?</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3580</PointerOffset>
      <VoiceId>16000065</VoiceId>
      <JapaneseText>……見ないで……見るな!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3625</PointerOffset>
      <VoiceId>16000066</VoiceId>
      <JapaneseText>こいつは、&lt;Red&gt;ハーフ&lt;White&gt;だ。
ヒューマでもガジュマでもない
混ざり物の出来損ないさ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3672</PointerOffset>
      <VoiceId>16000067</VoiceId>
      <JapaneseText>うそでしょ……？
ヒューマとガジュマの間の
子供なんて……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3703</PointerOffset>
      <VoiceId>16000068</VoiceId>
      <JapaneseText>うそも何も、こいつの頭を
見ればわかるだろう？
この角こそ、ヒューマとガジュマの
間に生まれた呪われし子の証だ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3783</PointerOffset>
      <VoiceId>16000069</VoiceId>
      <JapaneseText>確かに角……そして尻尾は
ハーフが持って生まれる
ものだと聞くが……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3812</PointerOffset>
      <VoiceId>1600006a</VoiceId>
      <JapaneseText>まさか、ハーフである事を
隠すために角を折ったというのか!?</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3849</PointerOffset>
      <VoiceId>1600006b</VoiceId>
      <JapaneseText>バカな女だ……
角がハーフのフォルスを
高めている事ぐらい、
わかっていただろうに。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3871</PointerOffset>
      <VoiceId>1600006c</VoiceId>
      <JapaneseText>まあ、それでも泣きつかれたから、
お情けで使ってやってたが、
もうおしまいだな。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3902</PointerOffset>
      <VoiceId>1600006d</VoiceId>
      <JapaneseText>そんな……私はまだやれる……
こいつらを……倒して……
純粋な&lt;Yellow&gt;ヒト&lt;White&gt;の身体をもらうんだ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3924</PointerOffset>
      <VoiceId>1600006e</VoiceId>
      <JapaneseText>純粋な身体をもらう？
馬鹿が！　生まれつきの身体を
変える事などできるものか！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3955</PointerOffset>
      <VoiceId>1600006f</VoiceId>
      <JapaneseText>そんな……
約束したはずよ、トーマ!!
こいつらを倒したら、
この憎き身体を捨てて……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3984</PointerOffset>
      <VoiceId>16000070</VoiceId>
      <JapaneseText>おまえはだまされているんだ、&lt;Hilda&gt;。
この男に……利用されているだけだ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4013</PointerOffset>
      <VoiceId>16000071</VoiceId>
      <JapaneseText>ふん！　角さえあれば、
楽しい夢を見ていられたものを！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4044</PointerOffset>
      <VoiceId>16000072</VoiceId>
      <JapaneseText>夢って……どういう事？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4066</PointerOffset>
      <VoiceId>16000073</VoiceId>
      <JapaneseText>ハーフの並外れた能力が欲しくて、
おまえをその気にさせただけだ。
角を折っても、そこそこやれるだろうと
思ったが、所詮は混ざり物だな。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4146</PointerOffset>
      <VoiceId>16000074</VoiceId>
      <JapaneseText>ひどい……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4168</PointerOffset>
      <VoiceId>16000075</VoiceId>
      <JapaneseText>だまされたほうが
悪いのだろう？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4225</PointerOffset>
      <VoiceId>16000076</VoiceId>
      <JapaneseText>……ひどい……ひどい……
……ひどい……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4363</PointerOffset>
      <VoiceId>16000077</VoiceId>
      <JapaneseText>まずい!!
暴走が始まるよ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4479</PointerOffset>
      <VoiceId>16000078</VoiceId>
      <JapaneseText>少しは役立ちそうだな。
ハーフの能力を見せてみろ、
&lt;Hilda&gt;!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4559</PointerOffset>
      <VoiceId>16000079</VoiceId>
      <JapaneseText>うわぁぁぁぁぁぁ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4590</PointerOffset>
      <VoiceId>1600007a</VoiceId>
      <JapaneseText>ふははははは……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>-1</SpeakerId>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4647</PointerOffset>
      <VoiceId>1600007b</VoiceId>
      <JapaneseText>待て、トーマ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4735</PointerOffset>
      <VoiceId>1600007c</VoiceId>
      <JapaneseText>うっ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4819</PointerOffset>
      <VoiceId>1600007d</VoiceId>
      <JapaneseText>&lt;Veigue&gt;、&lt;Hilda&gt;を
このままにしておくわけには
いかない！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4867</PointerOffset>
      <VoiceId>1600007e</VoiceId>
      <JapaneseText>……くっ！
そうか！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
