  <Strings>
    <Section>Story</Section>
    <Entry>
      <PointerOffset>651</PointerOffset>
      <JapaneseText>オアシス</JapaneseText>
      <EnglishText>Oasis</EnglishText>
      <Notes/>
      <Id>1</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>654</PointerOffset>
      <JapaneseText>オアシス</JapaneseText>
      <EnglishText>Oasis</EnglishText>
      <Notes/>
      <Id>2</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>1135</PointerOffset>
      <JapaneseText>&lt;Veigue&gt;</JapaneseText>
      <EnglishText>&lt;Veigue&gt;</EnglishText>
      <Notes/>
      <Id>3</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>1138</PointerOffset>
      <JapaneseText>&lt;voice:1a000093&gt;なんだ、あのバイラスは!?</JapaneseText>
      <EnglishText>&lt;voice:1A000093&gt;What's with that one over
there?!</EnglishText>
      <Notes/>
      <Id>4</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>1156</PointerOffset>
      <JapaneseText>&lt;Mao&gt;</JapaneseText>
      <EnglishText>&lt;Mao&gt;</EnglishText>
      <Notes/>
      <Id>5</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>1159</PointerOffset>
      <JapaneseText>&lt;voice:1a000094&gt;金色に光ってるヨ！</JapaneseText>
      <EnglishText>&lt;voice:1A000094&gt;It's glittering gold!</EnglishText>
      <Notes/>
      <Id>6</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>1178</PointerOffset>
      <JapaneseText>&lt;Tytree&gt;</JapaneseText>
      <EnglishText>&lt;Tytree&gt;</EnglishText>
      <Notes/>
      <Id>7</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>1181</PointerOffset>
      <JapaneseText>&lt;voice:1a000095&gt;……って事はヤツが!!</JapaneseText>
      <EnglishText>&lt;voice:1A000095&gt;Then that must be it...!</EnglishText>
      <Notes/>
      <Id>8</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>1214</PointerOffset>
      <JapaneseText>&lt;Eugene&gt;</JapaneseText>
      <EnglishText>&lt;Eugene&gt;</EnglishText>
      <Notes/>
      <Id>9</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>1217</PointerOffset>
      <JapaneseText>&lt;voice:1a000096&gt;必ず仕留めるんだ！</JapaneseText>
      <EnglishText>&lt;voice:1A000096&gt;Bring it down at all costs!</EnglishText>
      <Notes/>
      <Id>10</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>1430</PointerOffset>
      <JapaneseText>&lt;Eugene&gt;</JapaneseText>
      <EnglishText>&lt;Eugene&gt;</EnglishText>
      <Notes/>
      <Id>11</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>1433</PointerOffset>
      <JapaneseText>&lt;voice:1a000097&gt;今度こそ、リブガロを倒すぞ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1707</PointerOffset>
      <JapaneseText>&lt;Tytree&gt;</JapaneseText>
      <EnglishText>&lt;Tytree&gt;</EnglishText>
      <Notes/>
      <Id>13</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>1710</PointerOffset>
      <JapaneseText>&lt;voice:1a000098&gt;チクショウ……
目の前で逃がすなんて……
早くしないと&lt;Annie&gt;が！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1728</PointerOffset>
      <JapaneseText>&lt;Eugene&gt;</JapaneseText>
      <EnglishText>&lt;Eugene&gt;</EnglishText>
      <Notes/>
      <Id>15</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>1731</PointerOffset>
      <JapaneseText>&lt;voice:1a000099&gt;……追うぞ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2156</PointerOffset>
      <JapaneseText>&lt;Eugene&gt;</JapaneseText>
      <EnglishText>&lt;Eugene&gt;</EnglishText>
      <Notes/>
      <Id>17</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>2159</PointerOffset>
      <JapaneseText>&lt;voice:1a00009a&gt;やったぞ……リブガロの角だ!!</JapaneseText>
      <EnglishText>&lt;voice:1A00009A&gt;We did it... We have the
Livgaro's horn!</EnglishText>
      <Notes/>
      <Id>18</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2202</PointerOffset>
      <JapaneseText>&lt;Tytree&gt;</JapaneseText>
      <EnglishText>&lt;Tytree&gt;</EnglishText>
      <Notes/>
      <Id>19</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>2205</PointerOffset>
      <JapaneseText>&lt;voice:1a00009b&gt;これで&lt;Annie&gt;は助かるぜ！</JapaneseText>
      <EnglishText>&lt;voice:1A00009B&gt;Now we can save &lt;Annie&gt;!</EnglishText>
      <Notes/>
      <Id>20</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2347</PointerOffset>
      <JapaneseText>&lt;Mao&gt;</JapaneseText>
      <EnglishText>&lt;Mao&gt;</EnglishText>
      <Notes/>
      <Id>21</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>2350</PointerOffset>
      <JapaneseText>&lt;voice:1a00009c&gt;フランツさん、
早く、薬を作って!!</JapaneseText>
      <EnglishText>&lt;voice:1A00009C&gt;Frantz, hurry and make the
medicine!</EnglishText>
      <Notes/>
      <Id>22</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2384</PointerOffset>
      <JapaneseText>フランツ</JapaneseText>
      <EnglishText>Franz</EnglishText>
      <Notes/>
      <Id>23</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2387</PointerOffset>
      <JapaneseText>&lt;voice:1a00009d&gt;ここではできない……</JapaneseText>
      <EnglishText>&lt;voice:1A00009D&gt;I can't do it here...</EnglishText>
      <Notes/>
      <Id>24</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2472</PointerOffset>
      <JapaneseText>&lt;Tytree&gt;</JapaneseText>
      <EnglishText>&lt;Tytree&gt;</EnglishText>
      <Notes/>
      <Id>25</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>2475</PointerOffset>
      <JapaneseText>&lt;voice:1a00009e&gt;どうして!?</JapaneseText>
      <EnglishText>&lt;voice:1A00009E&gt;Why not?!</EnglishText>
      <Notes/>
      <Id>26</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2503</PointerOffset>
      <JapaneseText>フランツ</JapaneseText>
      <EnglishText>Franz</EnglishText>
      <Notes/>
      <Id>27</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2506</PointerOffset>
      <JapaneseText>&lt;voice:1a00009f&gt;角の中身を薬に使うのだが、
光に当てると成分が変化してしまう。
真っ暗な場所が必要だ。</JapaneseText>
      <EnglishText>&lt;voice:1A00009F&gt;Although the contents of the
horn are used to make the medicine,
the chemical components will change
if exposed to sunlight. It must be
done in total darkness.</EnglishText>
      <Notes/>
      <Id>28</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2540</PointerOffset>
      <JapaneseText>&lt;Veigue&gt;</JapaneseText>
      <EnglishText>&lt;Veigue&gt;</EnglishText>
      <Notes/>
      <Id>29</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>2543</PointerOffset>
      <JapaneseText>&lt;voice:1a0000a0&gt;真っ暗な場所か……
アニカマルで部屋を
借りるしかない。</JapaneseText>
      <EnglishText>&lt;voice:1A0000A0&gt;Total darkness... We'll have to
borrow a room somewhere in Anikamal.</EnglishText>
      <Notes/>
      <Id>30</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2576</PointerOffset>
      <JapaneseText>&lt;Mao&gt;</JapaneseText>
      <EnglishText>&lt;Mao&gt;</EnglishText>
      <Notes/>
      <Id>31</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>2579</PointerOffset>
      <JapaneseText>&lt;voice:1a0000a1&gt;でも、大丈夫かな？</JapaneseText>
      <EnglishText>&lt;voice:1A0000A1&gt;But will we be able to?</EnglishText>
      <Notes/>
      <Id>32</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2610</PointerOffset>
      <JapaneseText>&lt;Eugene&gt;</JapaneseText>
      <EnglishText>&lt;Eugene&gt;</EnglishText>
      <Notes/>
      <Id>33</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>2613</PointerOffset>
      <JapaneseText>&lt;voice:1a0000a2&gt;アニカマルへ戻ろう。
どんな事をしてでも
&lt;Annie&gt;を救う。</JapaneseText>
      <EnglishText>&lt;voice:1A0000A2&gt;Let's go back to Anikamal.
We're saving &lt;Annie&gt; no matter the
cost.</EnglishText>
      <Notes/>
      <Id>34</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>2645</PointerOffset>
      <JapaneseText>&lt;Veigue&gt;</JapaneseText>
      <EnglishText>&lt;Veigue&gt;</EnglishText>
      <Notes/>
      <Id>35</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>2648</PointerOffset>
      <JapaneseText>&lt;voice:1a0000a3&gt;よし、急ごう！</JapaneseText>
      <EnglishText>&lt;voice:1A0000A3&gt;All right, let's go.</EnglishText>
      <Notes/>
      <Id>36</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3026</PointerOffset>
      <JapaneseText>おばあさんの声</JapaneseText>
      <EnglishText>Grandma</EnglishText>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3042</PointerOffset>
      <JapaneseText>&lt;voice:3900000d&gt;た、助けてーっ！
</JapaneseText>
      <EnglishText>&lt;voice:3900000D&gt;H-heeeelp!!</EnglishText>
      <Notes/>
      <Id>38</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3150</PointerOffset>
      <JapaneseText>&lt;Veigue&gt;</JapaneseText>
      <EnglishText>&lt;Veigue&gt;</EnglishText>
      <Notes/>
      <Id>39</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>3153</PointerOffset>
      <JapaneseText>&lt;voice:3900000e&gt;いたぞ、あそこだ!!
</JapaneseText>
      <EnglishText>&lt;voice:3900000E&gt;They're over there!</EnglishText>
      <Notes/>
      <Id>40</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3560</PointerOffset>
      <JapaneseText>おじいさん</JapaneseText>
      <EnglishText>Old Man</EnglishText>
      <Notes/>
      <Id>41</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3568</PointerOffset>
      <JapaneseText>おばあさん</JapaneseText>
      <EnglishText>Grandma</EnglishText>
      <Notes/>
      <Id>42</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3614</PointerOffset>
      <JapaneseText>&lt;Veigue&gt;</JapaneseText>
      <EnglishText>&lt;Veigue&gt;</EnglishText>
      <Notes/>
      <Id>43</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>3617</PointerOffset>
      <JapaneseText>&lt;voice:3900000f&gt;みんな、大丈夫か？
</JapaneseText>
      <EnglishText>&lt;voice:3900000F&gt;Is everyone all right?</EnglishText>
      <Notes/>
      <Id>44</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3639</PointerOffset>
      <JapaneseText>&lt;voice:39000010&gt;助かったよ、ありがとう。
</JapaneseText>
      <EnglishText>&lt;voice:39000010&gt;Thank you for saving us.</EnglishText>
      <Notes/>
      <Id>45</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3678</PointerOffset>
      <JapaneseText>&lt;voice:39000011&gt;……ガジュマの連中め！
こんな砂漠に、
わしらを追い出しおって！
</JapaneseText>
      <EnglishText>&lt;voice:39000011&gt;...Those Gajuma thugs! They
just tossed us out into the desert!</EnglishText>
      <Notes/>
      <Id>46</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3700</PointerOffset>
      <JapaneseText>&lt;voice:39000012&gt;やっぱり、ここには住めないわ。
アニカマルに帰りたい……
</JapaneseText>
      <EnglishText>&lt;voice:39000012&gt;We can't live out here. I want
to go back to Anikamal...</EnglishText>
      <Notes/>
      <Id>47</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3726</PointerOffset>
      <JapaneseText>&lt;Eugene&gt;</JapaneseText>
      <EnglishText>&lt;Eugene&gt;</EnglishText>
      <Notes/>
      <Id>48</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>3729</PointerOffset>
      <JapaneseText>&lt;voice:39000013&gt;そうだ、帰ろう。
俺たちも一緒に説得をする。
</JapaneseText>
      <EnglishText>&lt;voice:39000013&gt;Yes, you should go back. We'll
go with you and try to reason with
them.</EnglishText>
      <Notes/>
      <Id>49</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3774</PointerOffset>
      <JapaneseText>&lt;voice:39000014&gt;ガジュマの言う事なんぞ……
</JapaneseText>
      <EnglishText>&lt;voice:39000014&gt;I'm supposed to trust the words
of a Gajuma?</EnglishText>
      <Notes/>
      <Id>50</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3804</PointerOffset>
      <JapaneseText>&lt;voice:39000015&gt;でも、この人たちは、
私たちを助けてくれたのよ……
</JapaneseText>
      <EnglishText>&lt;voice:39000015&gt;But these people did come and
save us...</EnglishText>
      <Notes/>
      <Id>51</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3826</PointerOffset>
      <JapaneseText>&lt;voice:39000016&gt;ガジュマとだって
仲良くできるはずよ……
だって、以前は……
</JapaneseText>
      <EnglishText>&lt;voice:39000016&gt;It should be possible to get
along with the Gajuma... After all,
we did so before...</EnglishText>
      <Notes/>
      <Id>52</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3863</PointerOffset>
      <JapaneseText>&lt;Tytree&gt;</JapaneseText>
      <EnglishText>&lt;Tytree&gt;</EnglishText>
      <Notes/>
      <Id>53</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>3866</PointerOffset>
      <JapaneseText>&lt;voice:39000017&gt;ばあさんの言うとおりだぜ。
前にできて、今できない
なんて事はないだろう？
</JapaneseText>
      <EnglishText>&lt;voice:39000017&gt;Yeah, she's right. If you could
all get along before, then why
can't you do it again?</EnglishText>
      <Notes/>
      <Id>54</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3912</PointerOffset>
      <JapaneseText>&lt;Tytree&gt;</JapaneseText>
      <EnglishText>&lt;Tytree&gt;</EnglishText>
      <Notes/>
      <Id>55</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>3915</PointerOffset>
      <JapaneseText>&lt;voice:39000018&gt;さ、アニカマルへ
帰ろうぜ！
</JapaneseText>
      <EnglishText>&lt;voice:39000018&gt;Come on, let's head on back to
Anikamal!</EnglishText>
      <Notes/>
      <Id>56</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>5442</PointerOffset>
      <JapaneseText>select</JapaneseText>
      <EnglishText>Select</EnglishText>
      <Notes/>
      <Id>57</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>5445</PointerOffset>
      <JapaneseText>&lt;speed:00&gt;&lt;scale:0140&gt;休憩しますか？
　はい　
　いいえ　</JapaneseText>
      <EnglishText>&lt;speed:00&gt;&lt;scale:0140&gt;Would you like to rest?
Yes
No</EnglishText>
      <Notes/>
      <Id>58</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>7113</PointerOffset>
      <JapaneseText>&lt;Green&gt;&lt;scale:0140&gt;&lt;speed:00&gt;&lt;font:80000002&gt;&lt;Unk19:3404&gt;が仲間になりました。</JapaneseText>
      <EnglishText>&lt;Green&gt;&lt;scale:0140&gt;&lt;speed:00&gt;&lt;Unk19:3404&gt; joined the party.</EnglishText>
      <Notes/>
      <Id>59</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>7191</PointerOffset>
      <JapaneseText>&lt;Green&gt;&lt;scale:0140&gt;&lt;speed:00&gt;&lt;font:80000002&gt;&lt;Unk19:3410&gt;が同行します。</JapaneseText>
      <EnglishText>&lt;Green&gt;&lt;scale:0140&gt;&lt;speed:00&gt;&lt;Unk19:3410&gt; is now travelling with you.</EnglishText>
      <Notes/>
      <Id>60</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>7287</PointerOffset>
      <JapaneseText>notice</JapaneseText>
      <EnglishText>Notice</EnglishText>
      <Notes/>
      <Id>61</Id>
      <Status>Editing</Status>
    </Entry>
  </Strings>
</SceneText>
