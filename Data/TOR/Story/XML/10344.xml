  <Strings>
    <Section>s2-1</Section>
    <Entry>
      <PointerOffset>2828</PointerOffset>
      <JapaneseText>……うぅ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2874</PointerOffset>
      <JapaneseText>……そっとしておいた
ほうがよさそうだな。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2910</PointerOffset>
      <JapaneseText>休ませてあげなきゃネ。</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2946</PointerOffset>
      <JapaneseText>今は、そっとしておく
べきだな……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2983</PointerOffset>
      <JapaneseText>…………
</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3020</PointerOffset>
      <JapaneseText>早くよくなれよ。
</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
