      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>100</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4186</PointerOffset>
      <VoiceId>1c0000f4</VoiceId>
      <JapaneseText>そう……でも、&lt;Claire&gt;さんは
