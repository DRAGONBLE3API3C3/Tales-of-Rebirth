      <EnglishText/>
      <Notes/>
      <SpeakerId>10,11,12,13,14,15,15</SpeakerId>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2885</PointerOffset>
      <VoiceId>1c0000cb</VoiceId>
      <JapaneseText>そ、それは……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>60</Id>
      <Status>To Do</Status>
