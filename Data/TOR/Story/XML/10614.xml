      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2673</PointerOffset>
      <VoiceId>1c0000bc</VoiceId>
      <JapaneseText>このままでは、どのみち
