      <SpeakerId>10,11,12,13,14,15,15</SpeakerId>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2608</PointerOffset>
      <VoiceId>1c0000b9</VoiceId>
      <JapaneseText>私も覚悟はできてます!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10,11,12,13,14,15,15</SpeakerId>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2630</PointerOffset>
      <VoiceId>1c0000ba</VoiceId>
