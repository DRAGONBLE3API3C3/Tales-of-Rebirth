      <EnglishText/>
      <Notes/>
      <SpeakerId>10,11,12,13,14,15,15,16</SpeakerId>
      <Id>99</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4164</PointerOffset>
      <VoiceId>1c0000f3</VoiceId>
      <JapaneseText>こいつの……
&lt;Veigue&gt;の恋……
