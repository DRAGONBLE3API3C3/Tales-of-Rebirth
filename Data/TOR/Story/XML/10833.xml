      <EnglishText/>
      <Notes/>
      <SpeakerId>0</SpeakerId>
      <Id>73</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5173</PointerOffset>
      <VoiceId>3f000011</VoiceId>
      <JapaneseText>田舎娘の姿をしたあなたに
