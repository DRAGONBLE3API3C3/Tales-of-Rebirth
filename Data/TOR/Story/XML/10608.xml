      <EnglishText/>
      <SpeakerId>1</SpeakerId>
      <Notes/>
      <Id>121</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2410</PointerOffset>
      <JapaneseText>ああ、がんばるさ！
