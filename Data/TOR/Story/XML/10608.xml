      <EnglishText/>
      <SpeakerId>1</SpeakerId>
      <Notes/>
      <Id>124</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2501</PointerOffset>
      <JapaneseText>あまり頼りにならない
