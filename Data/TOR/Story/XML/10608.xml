      <SpeakerId>0</SpeakerId>
      <Notes/>
      <Id>122</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2454</PointerOffset>
      <JapaneseText>私たちみんな、
