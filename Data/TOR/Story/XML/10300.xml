      <EnglishText/>
      <SpeakerId>2</SpeakerId>
      <Notes/>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>3413</PointerOffset>
      <VoiceId>27000018</VoiceId>
      <JapaneseText>あれ、ここは……？
