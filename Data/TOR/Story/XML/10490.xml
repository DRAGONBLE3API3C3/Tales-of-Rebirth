      <EnglishText/>
      <Notes/>
      <Id>118</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>5182</PointerOffset>
      <JapaneseText>もちろんミルハウスト将軍も
