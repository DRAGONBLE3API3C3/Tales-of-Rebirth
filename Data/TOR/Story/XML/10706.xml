</JapaneseText>
      <EnglishText/>
      <SpeakerId>1</SpeakerId>
      <Notes/>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>2824</PointerOffset>
