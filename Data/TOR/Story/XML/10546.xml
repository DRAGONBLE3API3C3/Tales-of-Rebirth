      <JapaneseText>キョグエン</JapaneseText>
      <EnglishText>Kyogen</EnglishText>
      <Notes/>
      <Id>2</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>660</PointerOffset>
