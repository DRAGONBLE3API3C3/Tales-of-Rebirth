      <Id>92</Id>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>3961</PointerOffset>
      <JapaneseText>&lt;Mao&gt;</JapaneseText>
      <EnglishText>&lt;Mao&gt;</EnglishText>
      <Notes/>
      <Id>93</Id>
      <Status>Done</Status>
    </Entry>
    <Entry>
      <PointerOffset>3964</PointerOffset>
      <JapaneseText>&lt;voice:2a00007c&gt;う、うん……</JapaneseText>
      <EnglishText>&lt;voice:2A00007C&gt;Um, yeah...</EnglishText>
      <Notes/>
      <Id>94</Id>
      <Status>Editing</Status>
    </Entry>
