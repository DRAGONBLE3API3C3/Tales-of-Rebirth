      <EnglishText/>
      <SpeakerId>0</SpeakerId>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>1567</PointerOffset>
      <JapaneseText>またね。</JapaneseText>
      <EnglishText/>
      <SpeakerId>0</SpeakerId>
      <Notes/>
