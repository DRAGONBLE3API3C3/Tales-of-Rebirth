    <Entry>
      <PointerOffset>657</PointerOffset>
      <JapaneseText>ヒューマに良い感情を
持った者はいないのでね。</JapaneseText>
      <EnglishText/>
      <SpeakerId>1</SpeakerId>
      <Notes/>
      <Id>10</Id>
