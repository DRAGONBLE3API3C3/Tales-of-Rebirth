      <EnglishText/>
      <SpeakerId>0</SpeakerId>
      <Notes/>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>4762</PointerOffset>
      <VoiceId>21000004</VoiceId>
